{
  "name": "pnpm-turborepo-tanstack",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo build",
    "build:dashboard": "turbo build --filter=@monorepo/dashboard",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "dev": "turbo dev --parallel",
    "dev:api": "turbo dev --filter=@monorepo/api",
    "dev:dashboard": "turbo dev --filter=@monorepo/dashboard",
    "dev:desktop": "turbo dev --filter=@monorepo/desktop",
    "dev:website": "turbo dev --filter=@monorepo/website",
    "format": "biome format --write .",
    "jobs:dashboard": "turbo jobs --filter=@monorepo/jobs",
    "start:dashboard": "turbo start --filter=@monorepo/dashboard",
    "start:website": "turbo start --filter=@monorepo/website",
    "test": "turbo test --parallel",
    "typecheck": "turbo typecheck",
    "lint": "turbo lint"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@monorepo/tsconfig": "workspace:*",
    "turbo": "^2.8.3",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "core-js-pure",
      "esbuild",
      "msw"
    ],
    "onlyBuiltDependencies": [
      "@biomejs/biome"
    ]
  }
}
